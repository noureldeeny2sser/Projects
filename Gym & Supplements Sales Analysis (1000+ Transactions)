        "    'Invoice_ID': [f\"INV-{i}\" for i in range(1000, 2000)],\n",
        "    'Branch': np.random.choice(branches, 1000),\n",
        "    'Product_Line': np.random.choice(products, 1000),\n",
        "    'Unit_Price': np.random.uniform(50, 2000, 1000).round(2),\n",
        "    'Quantity': np.random.randint(1, 10, 1000),\n",
        "    'Payment': np.random.choice(payments, 1000),\n",
        "    'Rating': np.random.uniform(2.0, 10.0, 1000).round(1)\n",
